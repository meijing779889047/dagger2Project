<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.dagger2.xeample1">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.ImplementWayOneActivity" />
        <activity android:name=".ui.ImplementWayTwoActivity" />
        <activity android:name=".ui.NamedActivity" />
        <activity android:name=".ui.QualifierActivity" />
        <activity android:name=".ui.SingletonActivity" />
        <activity android:name=".ui.ScopeActivity" />
        <activity android:name=".ui.DependienceActivity" />
        <activity android:name=".ui.LazyProviderActivity" />
        <activity android:name=".ui.SubcomponentActivity" />
        <activity android:name=".mvp.MvpActivity"></activity>
    </application>

</manifest>